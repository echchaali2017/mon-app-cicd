{
  "name": "mon-app-cicd",
  "version": "2.0.0",
  "description": "Application Node.js avec pipeline CI/CD compl√®te",
  "main": "app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node app.js",
    "test": "jest --config=jest.config.js --verbose --testTimeout=10000",
    "test:ci": "jest --config=jest.config.js --ci --coverage --verbose --testTimeout=10000",
    "test:watch": "jest --config=jest.config.js --watch",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix"
  },
  "keywords": ["nodejs", "express", "mongodb", "ci-cd", "docker"],
  "author": "Votre Nom",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^5.13.15",
    "cors": "^2.8.5",
    "helmet": "^6.1.5",
    "express-rate-limit": "^6.7.1"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.46.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
